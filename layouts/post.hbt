<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
{{> head }}
    <body class="post">
        {{> warning }}
        {{> header post=true}}
        <div class="main-container">
            <div class="main wrapper clearfix">
                <article>
                    {{{ contents }}}
                    {{#if featureimage}}
                        {{#if imageby}}
                            <p class="imagecitation">Title image by
                                <a href="{{imagelink}}" target="_blank">{{imageby}}</a>
                            </p>
                        {{/if}}
                    {{/if}}
                    {{> citation }}
                </article>
                <aside id="sidebar">
                    {{> nav }}
                    {{> postdata }}
                </aside>
            </div> <!-- #main -->
        </div> <!-- #main-container -->
        {{> footer }}
    </body>
</html>
