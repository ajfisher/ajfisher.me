{{> head }}
    <body class="list">
        {{> warning }}
        {{> header }}
        <div class="main-container">
            <div class="main wrapper clearfix">
                <article>
                    {{#each collections.posts}}
                        <li>
                            <h3>{{this.title}}</h3>
                            <h4>Posted {{this.date}} by {{this.author}}</h4>
                        </li>
                    {{/each}}
                </article>
                <aside>
                    <h3>aside</h3>
                    <p>blah blah</p>
                </aside>
            </div> <!-- #main -->
        </div> <!-- #main-container -->

        {{> footer }}
    </body>
</html>
