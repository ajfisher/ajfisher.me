<section id="postinfo">
    <h2>{{ title }}</h2>
    <p class="date">Published on {{ moment date 'dddd, MMMM Do YYYY' }}
    by {{ author }}</p>
    {{#if tags}}
        <p class="tags">Tagged:
        {{#each tags}}
            <a href="/tagged/{{this}}">{{this}}</a>
        {{/each}}
        </p>
    {{/if}}
</section>

